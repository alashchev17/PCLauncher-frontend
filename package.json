{
  "name": "gambit-launcher",
  "appId": "com.gambitrp.launcher",
  "productName": "Gambit Launcher",
  "version": "0.1.3",
  "description": "Лаунчер проекта Gambit Role Play",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "npx electron-builder --win nsis"
  },
  "keywords": [],
  "author": {
    "name": "Gambit Role Play",
    "email": "noreply@gambit-rp.ru"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@types/node": "^20.6.1",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "bufferutil": "^4.0.1",
    "css-loader": "^6.8.1",
    "electron": "26.2.1",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.4",
    "utf-8-validate": "^5.0.10"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.5",
    "ws": "^8.14.1"
  },
  "repository": "https://github.com/asfhjfwdsfwegwrgregfr/hshsggafasfas",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.gambitrp.launcher",
    "productName": "Gambit Launcher",
    "win": {
      "artifactName": "${name}-setup-${version}.${ext}",
      "target": "nsis",
      "icon": "./icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
